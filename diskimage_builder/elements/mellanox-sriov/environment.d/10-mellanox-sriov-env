#!/bin/bash

if [ ${DIB_DEBUG_TRACE:-1} -gt 0 ]; then
    set -x
fi
set -eu
set -o pipefail


DIB_ENABLE_SRIOV_IN_FIRMWARE=${DIB_ENABLE_SRIOV_IN_FIRMWARE:-0}
export DIB_ENABLE_SRIOV_IN_FIRMWARE

DIB_MLNX_SRIOV_VFS_NUM=${DIB_MLNX_SRIOV_VFS_NUM:-16}
export DIB_MLNX_SRIOV_VFS_NUM

DIB_CREATE_SRIOV_INTERFACES=${DIB_CREATE_SRIOV_INTERFACES:-1}
export DIB_CREATE_SRIOV_INTERFACES
